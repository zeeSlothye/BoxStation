    <None Remove="box\Reserved.xaml" />
    <None Remove="box\ReservedListPopup.xaml" />
    <None Remove="bus\Bus.xaml" />
    <None Remove="mypage\MyPage.xaml" />
    <None Remove="NaviPage.xaml" />
    <None Remove="ReservedList.xaml" />
  </ItemGroup>

  <ItemGroup>
